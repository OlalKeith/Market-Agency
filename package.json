{
  "name": "marketing-agency",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@tailwindcss/cli": "^4.0.6",
    "cra-template-typescript": "1.2.0",
    "flowbite": "^3.1.2",
    "framer-motion": "^12.4.3",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-calendly": "^4.4.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.5",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^4.0.6"
  },
  "scripts": {
    "start": "npm run build:css && react-scripts start",
    "build:css": "npx tailwindcss -i ./src/styles/global.css -o ./src/output.css",
    "build": "npx tailwindcss -i ./src/styles/global.css -o ./src/output.css --minify && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "web-vitals": "^4.2.4"
  }
}
